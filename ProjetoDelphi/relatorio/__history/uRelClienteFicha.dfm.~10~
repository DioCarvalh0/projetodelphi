  object qryClientes: TZQuery
    Connection = dtmPrincipal.ConexaoDB
    Active = True
    SQL.Strings = (
      'SELECT clienteId,'
      '      nome,'
      '      cep,'
      '      endereco,'
      '      bairro,'
      '      cidade,'
      '      estado,'
      '      email,'
      '      telefone,'
      '      dataNascimento'
      '  FROM clientes'
      '  ORDER BY nome'
      ''
      '')
    Params = <>
    Left = 584
    object qryClientesclienteId: TIntegerField
      FieldName = 'clienteId'
      ReadOnly = True
    end
